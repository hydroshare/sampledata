{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://sandbox.hydroshare.org/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#signin-menu > a"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "id_username"
      },
      "text": "creator"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "id_password"
      },
      "text": "creator"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.hl-btn.hl-btn-green"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "RESOURCES"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Create new"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "title"
      },
      "text": "test"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "select-file"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@class='form-horizontal']//button[.='Create Resource']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "btn-public"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "RESOURCES"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Create new"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "title"
      },
      "text": "private"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "select-file"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@class='form-horizontal']//button[.='Create Resource']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "RESOURCES"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "private"
    },
    {
      "type": "verifyTextPresent",
      "text": "test"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}