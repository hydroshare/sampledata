{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "${url}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#signin-menu > a"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "id_username"
      },
      "text": "${user_viewer}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "id_password"
      },
      "text": "${password_viewer}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.hl-btn.hl-btn-green"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.col-md-12 > p"
      }
    }
  ],
  "data": {
    "configs": {
      "manual": {
        "url": "http://sandbox.hydroshare.org",
        "user_viewer": "viewer",
        "password_viewer": "viewer"
      }
    },
    "source": "manual"
  },
  "inputs": [
    [
      "url",
      "string"
    ],
    [
      "user_viewer",
      "string"
    ],
    [
      "password_viewer",
      "string"
    ]
  ],
  "timeoutSeconds": 60
}